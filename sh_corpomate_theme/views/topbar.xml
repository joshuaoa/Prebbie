<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- topbar 1 start -->
	<template id="sh_corpomate_theme_style_top_bar_1" name="Top Bar Style 1" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
				<section id="sh_top_bar_1">
					<div class="container-fluid">
						<div class="sh_topbar_content">
							<div class="sh_tb_left">
								<div class="sh_main_content">
									<div class="sh_icon">
										<span class="fa fa-tachometer"></span>
									</div>
									<div class="sh_text">
										Raise your website traffic with invess
										<span>Free Site Analysis</span>
									</div>
								</div>
							</div>
							<div class="sh_tb_right">
								<div class="row">
									<div class="col-md-6">
										<div class="sh_contact_details bdr">
											<div class="sh_icon">
												<span class=" fa fa-phone"></span>
											</div>
											<div class="sh_content">
												<a href="+917984575681">	+91 <span style="padding-left:5px;">7984575681 </span></a>
											</div>
										</div>
									</div>
									<div class="col-md-6">
										<div class="sh_contact_details">
											<div class="sh_icon">
												<span class=" fa fa-envelope"></span>
											</div>
											<div class="sh_content">
												<a href="info@softhealer.com">	info@softhealer.com </a>
											</div>
										</div>
									</div>
								</div>

							</div>
						</div>
					</div>
				</section>
			<!-- End your code -->
		</xpath>
	</template>
	<!-- topbar 1 end -->

	<!-- topbar 2 start -->
	<template id="sh_corpomate_theme_style_top_bar_2" name="Top Bar Style 2" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
			<div class="oe_structure">
				<section id="sh_top_bar_2">
					<div class="container">
						<!-- Top Left -->
						<div class="top-left">
							<ul class="clearfix">
								<li>
									<a href="#">
										Bitcoin Rate  $1756.38
										<span class="fa fa-long-arrow-up"></span>
										17.65%
									</a>
								</li>
							</ul>
						</div>
						<!-- Top Right -->
						<div class="top-right">
							<!-- LANGUAGE SELECTOR -->
							<t t-set="flags" t-value="True" />
							<t t-call="website.language_selector">
								<t t-set="_div_classes" t-value="(_div_classes or '') + ' dropdown'" />
							</t>
						</div>
					</div>
				</section>
			</div>

			<!-- End your code -->
		</xpath>
	</template>
	<!-- topbar 2 end -->

	<!-- topbar 3 start-->
	<template id="sh_corpomate_theme_style_top_bar_3" name="Top Bar Style 3" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
			<section id="sh_top_bar_3">
				<div class="container">
					<div class="sh_topbar_content">
						<div class="sh_logo">
							<a class="navbar-brand" href="index.html">
								<img t-att-src="image_data_uri(website.logo)" id="logo-footer" class="img-fluid" alt="logo icon" style="" />
							</a>
						</div>
						<div class="sh_info">
							<div class="sh_call_info">
								<div class="sh_icon">
									<span class="fa fa-phone"></span>
								</div>
								<div class="sh_callus">
									<h4>call us on:</h4>
									<p>
										<a t-att-href="'tel: %s' %(res_company.phone)">
											<t t-esc="res_company.phone" />
										</a>
									</p>
								</div>
							</div>

							<div class="sh_call_info">
								<div class="sh_icon">
									<span class="fa fa-envelope"></span>
								</div>
								<div class="sh_callus">
									<h4>mail us on:</h4>
									<p>
										<a t-att-href="'mailto: %s' %(res_company.email)">
											<t t-esc="res_company.email" />
										</a>
									</p>
								</div>
							</div>

						</div>
					</div>
				</div>
			</section>
		</xpath>
	</template>
	<!-- topbar 3 end -->

	<!-- topbar 4 start-->
	<template id="sh_corpomate_theme_style_top_bar_4" name="Top Bar Style 4" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
			<!--  start your code from here -->
			<section id="sh_top_bar_4">
				<div class="container">
					<div class="sh_topbar_content">
						<div class="sh_left_content">
							<ul>
								<li>
									<a class="sh_btn" t-att-href="'mailto: %s' %(res_company.email)">
										<span class="fa fa-envelope" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[1]/ul[1]/li[2]/a[1]/span[1]"></span>
										<t t-esc="res_company.email"></t>
									</a>
								</li>
								<li>
									<a class="sh_btn" t-att-href="'tel: %s' %(res_company.phone)">
										<span class="fa fa-phone" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[1]/ul[1]/li[1]/a[1]/span[1]"></span>
										<t t-esc="res_company.phone"></t>
									</a>
								</li>
							</ul>
						</div>
						<div class="sh_right_content">
							<ul>
								<li>
									<a target="_blank" class="sh_btn" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[2]/ul[1]/li[1]/a[1]" href="https://www.facebook.com/Odoo">
										<span class="fa fa-facebook"></span>
									</a>
								</li>
								<li>
									<a target="_blank" class="sh_btn" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[2]/ul[1]/li[2]/a[1]" href="https://twitter.com/Odoo">
										<span class="fa fa-twitter"></span>
									</a>
								</li>
								<li>
									<a target="_blank" class="sh_btn" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[2]/ul[1]/li[3]/a[1]" href="https://www.pinterest.com/company/odoo">
										<span class="fa fa-pinterest-p"></span>
									</a>
								</li>
								<li>
									<a target="_blank" class="sh_btn" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[2]/ul[1]/li[3]/a[1]" href="https://www.linkedin.com/company/odoo">
										<span class="fa fa-linkedin"></span>
									</a>
								</li>
								<li>
									<a target="_blank" class="sh_btn" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[2]/ul[1]/li[4]/a[1]" href="https://www.youtube.com/user/OpenERPonline">
										<span class="fa fa-share-alt"></span>
									</a>
								</li>
							</ul>
						</div>

					</div>
				</div>
			</section>
			<!-- End your code -->
		</xpath>
	</template>
	<!-- topbar 4 end -->

	<!-- topbar 5 start -->
	<template id="sh_corpomate_theme_style_top_bar_5" name="Top Bar Style 5" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
			<!--  start your code from here -->
			<section id="sh_top_bar_5">
				<div class="container">
					<div class="sh_topbar_content">
						<div class="sh_left_content">
							<ul>
								<li>
									<a class="sh_btn">
										<span class="fa fa-clock-o"></span>
										9:00 AM to 7:00 PM
									</a>
								</li>
								<li>
									<a class="sh_btn" t-att-href="'mailto: %s' %(res_company.email)">
										<span class="fa fa-envelope" data-oe-model="ir.ui.view" data-oe-id="1648" data-oe-field="arch" data-oe-xpath="/data/xpath/section/div[1]/div[1]/div[1]/ul[1]/li[2]/a[1]/span[1]"></span>
										<t t-esc="res_company.email"></t>
									</a>
								</li>
							</ul>
						</div>

						<div class="sh_right_content">
							<ul>
								<li>
									<a href="#" class="sh_btn">FAQ</a>
								</li>

								<li>
									<a href="#" class="sh_btn">T&amp;C</a>
								</li>

								<li>
									<a href="#" class="sh_btn">Privacy Policy</a>
								</li>

							</ul>
						</div>
					</div>

				</div>

				<div class="sh_bottom_part">
					<div class="container">
						<div class="sh_main_part">
							<div class="sh_mail-box">
								<span class="fa fa-envelope"></span>
								<div class="sh_text">
									<h5>Mail us</h5>
									<p>
										<a class="sh_btn" t-att-href="'mailto: %s' %(res_company.email)">
											<t t-esc="res_company.email"></t>
										</a>
									</p>
								</div>
							</div>
							<div class="sh_mail-box">
								<span class="fa fa-phone"></span>
								<div class="sh_text">
									<h5>call us</h5>
									<p>
										<a class="sh_btn" t-att-href="'tel: %s' %(res_company.phone)">
											<t t-esc="res_company.phone"></t>
										</a>
									</p>
								</div>
							</div>
							<div class="sh_box_btn">
								<a href="#" class="btn btn-primary">request an appointment</a>
							</div>
						</div>
					</div>
				</div>


			</section>
			<!-- End your code -->
		</xpath>
	</template>
	<!-- topbar 5 end -->
</odoo>