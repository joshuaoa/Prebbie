<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="sh_corpomate_theme_style_copyright_1" name="Copyright 1" inherit_id="website.layout" active="False">
		<xpath expr="//footer" position="inside">
		  <section id="sh_copyright_1">
			<div class="container">
			  <div class="row">
				<div class="col-lg-6 col-md-6 col-sm-12 col-12 text-left font-arial sm-text-left">
				  <p class="muted">
					<span>Copyright © 2021 </span>
					<a t-att-href="'%s' %(res_company.website)" target="_blank" style="color:#212121;font-weight:600">
					  <t t-esc="res_company.name" />
					</a>
				  </p>
				</div>
				<div class="col-lg-6 col-md-6 col-sm-12 col-12">
				  <div class="m-t-10 ">
					<ul class="no-style fs-11 no-padding font-arial">
					  <li class="inline no-padding">
						<a href="/" class="link  p-l-10 p-r-10">Privacy Policy</a>
					  </li>
					  <li class="inline no-padding">
						<a href="/" class="link  p-l-10 p-r-10">Contact</a>
					  </li>
					  <li class="inline no-padding">
						<a href="/" class="link  p-l-10 p-r-10 xs-no-padding xs-m-t-10">Help</a>
					  </li>
					</ul>
				  </div>
				</div>
			  </div>
			</div>
		  </section>
		</xpath>
	  </template>

	  <template id="sh_corpomate_theme_style_copyright_2" name="Copyright 2" inherit_id="website.layout" active="False">
		<xpath expr="//footer" position="inside">
		  <section id="sh_copyright_2">
			<div class="container">
			  <div class="sh_main">
				<div class="sh_logo">
				  <div class="sh_inner_1">
					<a class="navbar-brand" href="index.html">
					  <img t-att-src="image_data_uri(website.logo)" id="logo-footer" class="img-fluid" alt="logo icon" style="width: 80%;" />
					</a>
					<span class="muted">
					  <span>Copyright © 2021</span>
					  <a t-att-href="'%s' %(res_company.website)" target="_blank" style="color:#858585;font-weight:600">
						<t t-esc="res_company.name" />
					  </a>
					  <span>.All Rights Reserved. </span>
					</span>
				  </div>
				</div>
				<div class="sh_line">
				  <hr>
				  </hr>
				</div>
				<div class="sh_social">
				  <ul>
					<li>
					  <a href="#">
						<span class="fa fa-facebook"></span>
					  </a>
					</li>
					<li>
					  <a href="#">
						<span class="fa fa-twitter"></span>
					  </a>
					</li>
					<li>
					  <a href="#">
						<span class="fa fa-pinterest-p"></span>
					  </a>
					</li>
					<li>
					  <a href="#">
						<span class="fa fa-share-alt"></span>
					  </a>
					</li>
				  </ul>
				</div>
			  </div>
			</div>
		  </section>
		</xpath>
	  </template>

	<template id="sh_corpomate_theme_style_copyright_3" name="Copyright 3" inherit_id="website.layout" active="False">
		<xpath expr="//footer" position="inside">
			<section id="sh_copyright_3">
				<div class=" copyright-section text-center">
					<p class="container-wide copyright">
						<span>Copyright © </span>
						<a t-att-href="'%s' %(res_company.website)">
							<t t-esc="res_company.name" />
						</a>
						<!-- LANGUAGE SELECTOR -->
						<t t-set="flags" t-value="True" />
						<t t-call="website.language_selector">
							<t t-set="_div_classes" t-value="(_div_classes or '') + ' dropup'" />
						</t>
					</p>
				</div>
			</section>
		</xpath>
	</template>

